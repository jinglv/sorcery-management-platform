:mode-path=\"language\" @getCode=\"getCode\" />\n        </div>\n      </el-col>\n      <el-col :span=\"12\">\n        <el-button size=\"small\" type=\"warning\" disabled>运行结果</el-button>\n        <div style=\"margin-top: 10px;\">\n          <ace-code-editor-dialog ref=\"ace\" v-model=\"result\" class=\"ace-editor\" :min-lines=\"20\" :mode-path=\"language\" />\n        </div>\n      </el-col>\n    </el-row>\n  </div>\n</template>\n\n<script>\nimport { run } from '@/api/httprunner'\nimport AceCodeEditorDialog from '@/components/HttpRunner/aceCodeEditorDialog'\n\nexport default {\n  components: {\n    AceCodeEditorDialog\n  },\n  props: {\n    code: {\n      type: String,\n      default: null\n    }\n  },\n  data() {\n    return {\n      language: 'python',\n      codes: '',\n      in_code: '',\n      result: '',\n      saveFlag: false\n    }\n  },\n  mounted() {\n    console.info('code:', this.code)\n    this.codes = this.code\n  },\n  methods: {\n    closeDialog() {\n      this.$emit('cancel', {})\n    },\n    getCode(code) {\n      this.in_code = code\n    },\n    // 运行\n    async runDebugtalk() {\n      console.info('run code:', this.code)\n      this.$emit('getInputCode', this.in_code)\n      // this.getCode()\n      const req = {\n        code: this.in_code === '' ? this.code : this.in_code\n      }\n      const resp = await run(req)\n      if (resp.success === true) {\n        this.result = resp.result\n      } else {\n        this.$message.error('执行失败！')\n      }\n    }\n  }\n}\n</script>\n<style scoped>\n  .el-row {\n    margin-bottom: 20px;\n    &:last-child {\n      margin-bottom: 0;\n    }\n  }\n  .el-col {\n    border-radius: 4px;\n  }\n</style>\n"]}]}