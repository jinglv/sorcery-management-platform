base_api_name }}</el-form-item>\n      <el-form-item label=\"接口详细信息：\" prop=\"api_info\">\n        <json-viewer :value=\"apiDetailForm.api_info\" :expand-depth=\"3\" />\n      </el-form-item>\n      <el-form-item style=\"text-align: right\">\n        <div class=\"dialog-footer\">\n          <el-button @click=\"closeDialog()\">取消</el-button>\n        </div>\n      </el-form-item>\n    </el-form>\n  </el-dialog>\n</template>\n\n<script>\nimport jsonViewer from 'vue-json-viewer'\nimport { httpRunnerApiDetail } from '@/api/httprunner'\n\nexport default {\n  components: {\n    jsonViewer\n  },\n  props: {\n    id: {\n      type: Number,\n      default: 0\n    }\n  },\n  data() {\n    return {\n      showTitle: '查看接口详情',\n      dialogVisible: true,\n      apiDetailForm: {\n        name: '',\n        base_api_name: '',\n        api_info: {}\n      }\n    }\n  },\n  mounted() {\n    this.httpRunnerApiDetail()\n  },\n  methods: {\n    closeDialog() {\n      this.$emit('cancel', {})\n    },\n    async httpRunnerApiDetail() {\n      const resp = await httpRunnerApiDetail(this.id)\n      if (resp.success === true) {\n        this.apiDetailForm = resp.result\n        // this.$message.success('查询成功！')\n      } else {\n        this.$message.error('查询失败！')\n      }\n    }\n  }\n}\n</script>\n<style scoped>\n#image {\n  text-align: left;\n}\n</style>\n"]}]}